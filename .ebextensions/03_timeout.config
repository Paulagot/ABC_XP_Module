---
option_settings:
  aws:elasticbeanstalk:command:
    Timeout: 1800
files:
  /etc/nginx/conf.d/timeout.conf:
    mode: "000644"
    owner: root
    group: root
    content: |
      proxy_connect_timeout 300;
      proxy_send_timeout 300;
      proxy_read_timeout 300;
      send_timeout 300;
      client_max_body_size 20M;


    